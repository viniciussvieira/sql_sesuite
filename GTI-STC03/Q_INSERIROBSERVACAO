INSERT INTO
    DYNGTISTC03OBERSAC (
        OID,
        NRVERSION,
        BNCREATED,
        FGENABLED,
        OIDREVISIONFORM,
        FGSYSTEM,
        BNUPDATED,
        NMUSERUPDATE,
        TXOBSERVACAO,
        NMUSUARIO,
        DTOBSERVACAO,
        OIDABCGNAYNXM7OYDT
    )
SELECT
    LEFT(
        REPLACE(
            CAST(
                (
                    UUID_IN(
                        CAST(
                            MD5(CAST(RANDOM() AS TEXT) || CAST(NOW() AS TEXT)) AS CSTRING
                        )
                    )
                ) AS VARCHAR
            ),
            '-',
            ''
        ),
        32
    ),
    NULL,
    NULL,
    NULL,
    1,
    NULL,
    NULL,
    :NMUSERUPDATE AS NMUSERUPDATE,
    :TXOBSERVACAO AS OBSERVACAO,
    :NMUSUARIO AS SOLICITANTE,
    NOW() AS DATA,
    (
        SELECT
            FR2.OIDENTITYREG
        FROM
            WFPROCESS WF2
            INNER JOIN GNASSOCFORMREG FR2 ON (
                WF2.CDASSOCREG = FR2.CDASSOC
            )
            INNER JOIN EMENTITYMODEL ENT ON (
                FR2.OIDENTITYMODEL = ENT.OID
            )
        WHERE
            ENT.IDNAME = 'GTISTC03'
            AND WF2.IDPROCESS = :ID
    ) AS OIDABCGNAYNXM7OYDT